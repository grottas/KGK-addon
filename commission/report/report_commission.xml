<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <template id="commission.commission_summary_report">
      <t t-call="web.html_container">
        <t t-call="web.internal_layout">
          <div class="page">
            <div class="row bg-primary">
              <div class="col-xs-1">Date</div>
              <div class="col-xs-2">Agent</div>
            </div>
            <t t-foreach="docs" t-as="o">
              <div class="row">
                <div class="col-xs-1">
                  <span t-field="o.start_date" />
                </div>
                <div class="col-xs-2">
                  <span t-field="o.sales_agent" />
                </div>
              </div>>
            </t>
          </div>
        </t>
      </t>
    </template>

    <report id="action_commission_summary_report"
      string="Commission summary report"
      report_type="qweb-html"
      model="commission.summary"
      name="commission.commission_summary_report"
    />
  </data>
</odoo>